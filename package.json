{
  "name": "custom-viewer-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "viewers": [
    {
      "type": "file",
      "title": "Marquee Viewer",
      "description": "A ridiculous marquee viewer",
      "entry": "/viewers/marquee-viewer/index.tsx"
    },
    {
      "type": "file",
      "title": "Chart Viewer",
      "description": "A ridiculous chart viewer",
      "entry": "/viewers/chart-viewer/index.tsx"
    },
    {
      "type": "file",
      "title": "Upside-Down Viewer",
      "description": "A ridiculous upside-down viewer",
      "entry": "/viewers/upside-down-viewer/index.tsx"
    },
    {
      "type": "folder",
      "title": "Folder Viewer",
      "description": "A ridiculous upside-down viewer",
      "entry": "/viewers/folder-viewer/index.tsx"
    }
  ],
  "dependencies": {
    "@codesandbox/sandpack-react": "^0.1.20",
    "@octokit/rest": "^18.12.0",
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "parse-github-url": "^1.0.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.4",
    "react-fast-marquee": "^1.2.1",
    "react-query": "^3.32.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/lodash": "^4.14.176",
    "@types/parse-github-url": "^1.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}
